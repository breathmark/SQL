# Join Inventory and Film
# What are most available films in stock by store id

phpMyAdmin demo - MySQL/sakila/inventory/		https://demo.phpmyadmin.net/master-config/public/index.php?route=/sql&db=sakila&table=inventory

   Showing rows 0 - 49 (525 total, Query took 0.0066 seconds.)


with most as(

select count(*) as total, f.title,i.store_id, 
	rank() over (partition by i.store_id order by count(*) desc) as ranking   				
    from inventory i
    join film f
    on i.film_id = f.film_id
    group by i.store_id, f.title)
    
select * from most
where ranking = 1
order by store_id DESC;


